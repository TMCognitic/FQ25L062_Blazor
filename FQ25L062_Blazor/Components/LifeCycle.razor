@implements IDisposable

<h3>LifeCycle</h3>

@if(_ints is not null)
{
    <ul>
        @foreach(int i in _ints)
        {
            <li>@i</li>
        }
    </ul>
}


@code 
{
    private List<int>? _ints;    

    protected override void OnInitialized()
    {
        //Abonner à un délégué ici

        Console.WriteLine("OnInitialized");
        _ints = new() { 1, 2, 3, 4, 5, 6, 7, 8, 9 };
    }

    public void Dispose()
    {
        //Désabonnement à un délégué ici
        Console.WriteLine("Dispose");
        if (_ints is not null)
            _ints.Clear();
        _ints = null;
    }
}
